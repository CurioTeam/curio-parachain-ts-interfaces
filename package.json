{
  "name": "curio-parachain-ts-interfaces",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "yarn generate:defs && yarn generate:meta",
    "generate:defs": "node --experimental-specifier-resolution=node --loader ts-node/esm node_modules/.bin/polkadot-types-from-defs --package curio-parachain-ts-interfaces/interfaces --input ./src/interfaces --endpoint ./curio.json",
    "generate:meta": "node --experimental-specifier-resolution=node --loader ts-node/esm node_modules/.bin/polkadot-types-from-chain --package curio-parachain-ts-interfaces/interfaces --endpoint ./curio.json --output ./src/interfaces",
    "lint": "tsc --noEmit --pretty"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@polkadot/typegen": "^10.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@polkadot/api": "^10.4.1"
  }
}
